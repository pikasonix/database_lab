<div class='mt-4'>
    <table class='table table-hover align-middle'>
        <thead class="text-center">
            <tr>
                {{#each test.[0]}}
                    <th>{{@key}}</th>
                {{/each}}
            </tr>
        </thead>
        <tbody class="table-group-divider">
            <tr>
                <td>{{test.[0].id}}</td>
            </tr>
            {{#eachFromIndex test 2}}
            <tr>
                <td>{{this.id}}</td>
            </tr>
            {{/eachFromIndex}}
        </tbody>
    </table>
</div>

<div class='row row-cols-1 row-cols-md-4 g-4'>
    <div class='col'>
        <div class='card h-100'>
            <img
                src='https://cdn2.cellphones.com.vn/insecure/rs:fill:358:358/q:90/plain/https://cellphones.com.vn/media/catalog/product/s/s/ss-s24-ultra-xam-222.png'
                class='card-img-top'
            />
            <div class='card-body'>
                <h6 class='card-title'>Samsung S24 Ultra</h6>
                <p class='card-text'>
                    <div class='text-danger me-2'><b>28.490.000 VND</b></div>
                    <div class='text-secondary'><small><s>33.990.000 VND</s></small></div>
                </p>

            </div>
            <div class='card-footer text-body-secondary text-center'>
                <a
                    href='#'
                    class='btn btn-sm btn-primary'
                    style='width: 40%'
                >Edit</a>
                <a
                    href='#'
                    class='btn btn-sm btn-danger'
                    style='width: 40%'
                >Remove</a>
            </div>
        </div>
    </div>
</div>